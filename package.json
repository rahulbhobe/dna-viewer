{
  "name": "dna-viewer",
  "version": "1.0.0",
  "description": "",
  "main": "server/babel/main.js",
  "scripts": {
    "start": "node server/index.js",
    "postinstall": "bower install && npm run build",
    "build": "npm run client-build & npm run server-build",
    "client-build": "browserify            -t [ babelify --presets [ react es2015 ] --plugins [ transform-object-rest-spread ] ] www/react/* www/store/* www/src/* -o www/bundle/dna_viewer_bundle.js",
    "client-watch": "watchify   --debug -v -t [ babelify --presets [ react es2015 ] --plugins [ transform-object-rest-spread ] ] www/react/* www/store/* www/src/* -o www/bundle/dna_viewer_bundle.js",
    "server-build": "babel         --presets es2015 server/src --out-dir server/babel",
    "server-watch": "babel --watch --presets es2015 server/src --out-dir server/babel"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.1",
    "bootstrap": "^3.3.6",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "html": "0.0.10",
    "jss": "^3.11.1",
    "mongoose": "^4.4.17",
    "react": "^15.0.2",
    "react-color": "^2.1.0",
    "react-dom": "^15.0.2",
    "redux": "^3.5.2",
    "request": "^2.72.0",
    "serve-favicon": "^2.3.0",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0"
  }
}
